{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-universal-file-previewer": {
      "projectType": "library",
      "root": "projects/ng-universal-file-previewer",
      "sourceRoot": "projects/ng-universal-file-previewer/src",
      "prefix": "ngx",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ng-universal-file-previewer/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ng-universal-file-previewer/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ng-universal-file-previewer/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ng-universal-file-previewer/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        }
      }
    },
    "ng-universal-file-previewer-demo": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/ng-universal-file-previewer-demo",
      "sourceRoot": "projects/ng-universal-file-previewer-demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ng-universal-file-previewer-demo",
            "index": "projects/ng-universal-file-previewer-demo/src/index.html",
            "main": "projects/ng-universal-file-previewer-demo/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/ng-universal-file-previewer-demo/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-universal-file-previewer-demo/src/favicon.ico",
              "projects/ng-universal-file-previewer-demo/src/assets"
            ],
            "styles": [
              "projects/ng-universal-file-previewer-demo/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "50kb",
                  "maximumError": "100kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ng-universal-file-previewer-demo:build:production"
            },
            "development": {
              "browserTarget": "ng-universal-file-previewer-demo:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ng-universal-file-previewer-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/ng-universal-file-previewer-demo/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/ng-universal-file-previewer-demo/src/favicon.ico",
              "projects/ng-universal-file-previewer-demo/src/assets"
            ],
            "styles": [
              "projects/ng-universal-file-previewer-demo/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
